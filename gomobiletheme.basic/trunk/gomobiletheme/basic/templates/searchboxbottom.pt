<div id="search-box-bottom" class="clear"  tal:condition="not: context/@@mobile_tool/isLowEndPhone" i18n:domain="gomobiletheme.basic">
	<h2 i18n:translate="">Search</h2>
	<form action="search">
		<table width="100%" cellspacing="0" cellpadding="0">
			<tbody>
				<tr>
					<td class="searchBar">
						<input  id="searchboxbottom"
								type="text" 
								name="SearchableText" 
								title="Search Site" 
								i18n:attributes="title title_search_title;"
								value=""
								class="searchfield" />
					</td>
					<td class="searchBtn">
						<input type="submit" value="Go" class="button" i18n:attributes="value search_go_button;" />
					</td>
				</tr>
			</tbody>
		</table>
	</form>
</div>
