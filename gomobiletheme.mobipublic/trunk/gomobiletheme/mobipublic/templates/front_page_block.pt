<div class="front-page-block" tal:define="master view/getMasterItem">

        <h2>
        	<a tal:attributes="href string:${master/absolute_url}" tal:content="master/Title" />
        </h2>

        <div class="highlights">
                <tal:item repeat="item view/getItems">
                        <a tal:attributes="href item/getURL" tal:content="item/Title" />
                </tal:item>         
		
		<div style="clear: both" />       
        </div>
	
	<div class="items">
		<tal:item repeat="item view/getSlots">
			<a tal:attributes="href item/absolute_url" tal:content="item/Title" />
		</tal:item>
		
		<div style="clear: both" />
	</div>
</div>

