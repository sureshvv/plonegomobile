<div id="footer-links"
    tal:define="
    	accesskeys python: {'sitemap' : '3', 'accessibility' : '0', 'contact' : '9'};    
	    getIconFor nocall:context/plone_utils/getIconFor;"
    i18n:domain="plone"    
    >
    				
	<ul id="portal-globalnav" tal:condition="python:len(view.portal_tabs) > 0">
        <tal:tabs tal:repeat="tab view/portal_tabs">
        	<li tal:attributes="id string:portaltab-${tab/id};">
        		<a href="" 
	               tal:content="tab/name"
	               tal:attributes="href tab/url;
                               title tab/description|nothing">
    		        Tab Name                       
            	</a>
	            |
            </li>					
		</tal:tabs>
		
		<li>
			<a tal:attributes="href string:${view/portal_url}/search">
				Search
			</a>
			|
		</li>

		<li>
			<a tal:attributes="href string:${view/portal_url}/contact-info">
				Feedback
			</a>
		</li>
		
    </ul>		
	    		

	<p id="copyright">
	    &copy; GoMobile Community
	    
	    <span 
	           tal:omit-tag=""
	           tal:define="now modules/DateTime/DateTime"
	           tal:content="now/year" />
	
		|
	    <a href="http://plone.org">
			plone.org web portal
	    </a>
	
	</p>

</div>